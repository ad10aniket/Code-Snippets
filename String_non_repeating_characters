public class Main {
    public static void main(String[] args) {
      String s = "appkleialezk";
      Set<Character> set = new TreeSet<>();
      for(char ch : s.toCharArray()){
        set.add(ch);
      }
      StringBuilder sb = new StringBuilder();
      for(char c : set){
        sb.append(c);
        System.out.print(c);
      }
  }
}
