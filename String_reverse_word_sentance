public class Main {
    public static void main(String[] args) {
      String sen = " Java is good for code";
      String [] arr = sen.trim().split("\\s+");
      StringBuilder sb = new StringBuilder(); 
      for(int i = arr.length -1;i>=0;i--){
        sb.append(arr[i]);
        if(i>0){
          sb.append(" ");
        }
      }

      System.out.println(sb);
  }
}
