public class Main {
    public static void main(String[] args) {
      String sen = "appkleialezk";
      Map<Character, Integer> map = new HashMap<>();
      for(char ch : sen.toCharArray()){
        map.put(ch, map.getOrDefault(ch,0)+1);
      }
      
      for(char c : map.keySet()){
        if(map.get(c) == 1)
        System.out.println(c);
      }
  }
}
